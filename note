    for i, row in data.iterrows():
        if row['installation_id'] != last_id or row['game_session'] != last_session:
            all_a += 1
            last_id = row['installation_id']
            last_session = row['game_session']
            if all_a % 1000 == 0:
                print(all_a)
    print(all_a)
